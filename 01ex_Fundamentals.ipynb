    "#define factorial function\n",
    "\n",
    "def fact_func_b(x):\n",
    "  \n",
    "    factorial = 1\n",
    "    #first value\n",
    "    if x < 0:\n",
    "        print(\"[N/A]\")\n",
    "    elif x == 0:\n",
    "        print([1])\n",
    "    else:\n",
    "        for i in range(1,x + 1):\n",
    "            factorial = factorial*i\n",
    "        print(factorial)\n",
    "\n",
    "fact_func_b(5)"
